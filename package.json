{
  "name": "activity-11-quest-api",
  "version": "1.0.0",
  "description": "Quest Tracker API - Express.js Backend for W3.5 Activity 11",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "npm run test:endpoints && npm run test:auth",
    "test:endpoints": "curl -X GET http://localhost:3000/api/quests?api_key=demo_key_12345",
    "test:auth": "curl -X GET http://localhost:3000/api/quests/1 -H 'X-API-Key: demo_key_12345'",
    "health": "curl http://localhost:3000/health"
  },
  "keywords": [
    "express",
    "api",
    "rest",
    "quest",
    "todo",
    "backend",
    "w3.5"
  ],
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "stackblitz": {
    "startCommand": "npm start"
  }
}